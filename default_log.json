{
    "logFolderPath": "./logs",
    "global": true,
    "logTypes": {
        "info": {
            "writeToFolder": true,
            "logTypePrimaryColour": "#D3D3D3",
            "logTypeBackgroundColour": null,
            "dataPrimaryColour": "#333333",
            "dataBackgroundColour": null
        },
        "warning": {
            "writeToFolder": true,
            "logTypePrimaryColour": "FFA500",
            "logTypeBackgroundColour": null,
            "dataPrimaryColour": "#333333",
            "dataBackgroundColour": null
        },
        "debug": {
            "writeToFolder": false,
            "logTypePrimaryColour": "FF6EC7",
            "logTypeBackgroundColour": null,
            "dataPrimaryColour": "#333333",
            "dataBackgroundColour": null
        },
        "importantLog": {
            "writeToFolder": true,
            "logTypePrimaryColour": "F5CC16",
            "logTypeBackgroundColour": null,
            "dataPrimaryColour": "#333333",
            "dataBackgroundColour": null
        },
        "error": {
            "writeToFolder": true,
            "logTypePrimaryColour": "FF0000",
            "logTypeBackgroundColour": null,
            "dataPrimaryColour": "#333333",
            "dataBackgroundColour": null
        }
    }
}